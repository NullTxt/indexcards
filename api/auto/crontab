# Automatic publication of round results
* * * * * /usr/bin/node /www/indexcards/api/auto/publishResults.js
* * * * * /usr/bin/node /www/indexcards/api/auto/autoBlastPairings.js

# Clear out old sharenames once a week to prevent name collisions
15 8 * * 4 /usr/bin/node /www/indexcards/api/auto/clearShareRooms.js

# NAUDL post to Salesforce
15 3 * * * /usr/bin/node /www/indexcards/api/auto/naudl/chapters.js
25 3 * * * /usr/bin/node /www/indexcards/api/auto/naudl/students.js
